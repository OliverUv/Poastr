<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:VerticalPanel addStyleNames="logIn_Panel">
		<g:HorizontalPanel addStyleNames="logIn_buttonPanel" ui:field="logIn_buttonPanel">
				<g:Image styleName="questionMark" ui:field="logIn_logInInfo"/>
				<g:Button styleName="logIn_Button" text="Log in" ui:field="loginLogoutButton"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 