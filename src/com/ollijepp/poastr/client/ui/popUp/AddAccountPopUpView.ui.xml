<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:DialogBox addStyleNames="popUp_DialogBox" ui:field="popUp_DialogBox">
			<g:VerticalPanel styleName="popUp_Panel" addStyleNames="DefaultColor" ui:field="popUp_Panel">
				<g:HorizontalPanel addStyleNames="addPopUp_socialServiceLogosPanel" ui:field="addPopUp_socialServiceLogosPanel" />
				<g:Label addStyleNames="addPopUp_Label" text="Username" ui:field="addPopUp_usernameLabel"/>
				<g:TextBox addStyleNames="addPopUp_TextBox" ui:field="addPopUp_usernameTextBox" />
				<g:Label addStyleNames="addPopUp_Label" text="Password" ui:field="addPopUp_passwordLabel"/>
				<g:PasswordTextBox addStyleNames="addPopUp_TextBox" ui:field="addPopUp_PasswordTextBox"/>
				<g:HorizontalPanel addStyleNames="addPopUp_ButtonPanel">
					<g:Button styleName="addPopUp_addAccountButton" text="Add Account" ui:field="addPopUp_addAccountButton"/>
					<g:Button styleName="popUp_closeButton" text="Close" ui:field="popUp_closeButton" />
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:DialogBox>
	</g:HTMLPanel>
</ui:UiBinder> 